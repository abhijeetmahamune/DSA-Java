class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int i = 0;
        int max = 0;
        for(int num : nums)
        {
            if(num == 1){
                i++;
            }else{ 
                i = 0;
            }
            if(i>max){
                    max = i;
            }
            // max = Math.max(i,max); this is best optimized code that i sisnt figured out but i searched on Gemini
        }
        return max; 
    }
}
